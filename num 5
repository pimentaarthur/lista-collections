using System;
using System.Collections;


    class program{

    static void Main(String [] args){

        int total=0;

        ArrayList arrayList = new ArrayList();

          for (int i = 1; i <= 100; i++) {
                arrayList.Add(i);   
        }  

       int count=0;
        do{
        total +=(int) arrayList[count];
        count++;

       }
       while(count<arrayList.Count);
        
       

       Console.WriteLine(total);

    
}
}

