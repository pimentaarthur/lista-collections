using System;
using System.Collections;


    class program{

    static void Main(String [] args){

        int total=0;

        ArrayList arrayList = new ArrayList();

          for (int i = 1; i <= 100; i++) {
            if (i % 2 == 1) {
                arrayList.Add(i);
            }
        }  

       for(int i=0; i<arrayList.Count; i++){
        
        total += (int)arrayList[i];
       }

       Console.WriteLine(total);

    
}
}

